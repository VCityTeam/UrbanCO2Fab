                           owl:onProperty     bldg:BuildingConstructiveElement.adeOfBuildingConstructiveElement
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  owl:Class ;
                           owl:onProperty     bldg:BuildingConstructiveElement.usage
