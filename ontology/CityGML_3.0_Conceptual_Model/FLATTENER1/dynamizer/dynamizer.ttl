                           owl:onProperty     dyn:SensorConnection.datastreamID
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  xsd:string ;
                           owl:onProperty     dyn:SensorConnection.sensorName
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  xsd:string ;
                           owl:onProperty     dyn:SensorConnection.observationProperty
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  owl:Class ;
                           owl:onProperty     dyn:SensorConnection.connectionType
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  xsd:string ;
                           owl:onProperty     dyn:SensorConnection.observationID
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  core:AbstractCityObject ;
                           owl:onProperty     dyn:SensorConnection.sensorLocation
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  owl:Class ;
                           owl:onProperty     dyn:SensorConnection.authType
