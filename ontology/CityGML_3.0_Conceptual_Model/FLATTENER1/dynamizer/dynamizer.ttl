dyn:TabulatedFileTimeseries
        a                owl:Class ;
        rdfs:subClassOf  dyn:AbstractAtomicTimeseries ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  xsd:integer ;
                           owl:onProperty     dyn:TabulatedFileTimeseries.idColumnNo
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  xsd:integer ;
                           owl:onProperty     dyn:TabulatedFileTimeseries.valueColumnNo
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  xsd:string ;
                           owl:onProperty     dyn:TabulatedFileTimeseries.idColumnName
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  owl:Class ;
                           owl:onProperty     dyn:TabulatedFileTimeseries.mimeType
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  dyn:ADEOfTabulatedFileTimeseries ;
                           owl:onProperty     dyn:TabulatedFileTimeseries.adeOfTabulatedFileTimeseries
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  xsd:string ;
                           owl:onProperty     dyn:TabulatedFileTimeseries.valueColumnName
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  owl:Class ;
                           owl:onProperty     dyn:TabulatedFileTimeseries.fileType
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  xsd:anyURI ;
                           owl:onProperty     dyn:TabulatedFileTimeseries.fileLocation
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  xsd:integer ;
                           owl:onProperty     dyn:TabulatedFileTimeseries.timeColumnNo
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  xsd:string ;
                           owl:onProperty     dyn:TabulatedFileTimeseries.fieldSeparator
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  xsd:integer ;
                           owl:onProperty     dyn:TabulatedFileTimeseries.numberOfHeaderLines
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  xsd:string ;
                           owl:onProperty     dyn:TabulatedFileTimeseries.timeColumnName
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  xsd:string ;
                           owl:onProperty     dyn:TabulatedFileTimeseries.idValue
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  dyn:TimeseriesTypeValue ;
                           owl:onProperty     dyn:TabulatedFileTimeseries.valueType
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  xsd:string ;
                           owl:onProperty     dyn:TabulatedFileTimeseries.decimalSymbol
                         ] ;
        skos:definition  "A TabulatedFileTimeseries represents time-varying data of a specific data type for a single contiguous time interval. The data is provided in an external file referenced in the TabulatedFileTimeseries. The file shall contain table structured data using an appropriate file format like comma separated values (CSV), Microsoft Excel (XLSX) or Google Spreadsheet. The timestamps and the values are given in specific columns of the table. Each row represents a single time-value-pair. A subset of rows can be selected using the idColumn and idValue attributes."@en ;
        skos:prefLabel   "TabulatedFileTimeseries"@en .

